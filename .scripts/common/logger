#!/bin/bash
#set -xuevo pipefail
. $(dirname $(readlink -f "${BASH_SOURCE[0]}"))/functions

totalErrors=0

logOk() {
   echoColor GREEN "$USERNAME/$TASKNAME: $*"
   #((totalErrors += 1))
}

logWarn() {
   echoColor YELLOW "$USERNAME/$TASKNAME: $*"
}

logError() {
   echoColor RED "$USERNAME/$TASKNAME: $*"
   ((totalErrors += 1))
}

